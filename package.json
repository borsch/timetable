  "description": "Timetable app to manage schedule of all faculties",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "run": "electron .",
    "package-win": "electron-packager . timatable --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Timetable\"",
    "package-linux": "electron-packager . timatable --overwrite --asar=true --platform=linux --arch=ia32 --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Timetable\""
  },
  "devDependencies": {
    "electron": "^1.7.8",
    "electron-packager": "^8.1.0"
  },
  "dependencies": {
    "jsonfile": "^4.0.0",
    "node-xlsx": "^0.11.2"
  }
}
